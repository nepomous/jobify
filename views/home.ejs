        <%- include('header')%> <!--chamou o arquivo de header e economizou codigo-->      
           
        <h2>Open Roles</h2>
        <% categorias.forEach( categoria => { %>
        <div>
            <h3><%- categoria.categoria %></h3>
            <ul>
                <% categoria.vagas.forEach( vaga => { %>
                <li><a href="Vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
                <% }) %>
            </ul>
        </div>
        <% }) %>

        <%- include('footer')%> <!--chamou o arquivo de footer e economizou codigo-->