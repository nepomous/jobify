        <%- include('header')%> <!--chamou o arquivo de header e economizou codigo-->      
           
        <h2>
            Admin: Vagas
            <a href="/admin/vagas/nova">Nova Vaga</a>
        </h2>
        <div>
            <ul>
                <% vagas.forEach( vaga => { %>
                <li>
                    <%- vaga.titulo %> | 
                    <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a> |
                    <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
                </li>
                <% }) %>
            </ul>
        </div>

        <%- include('footer')%> <!--chamou o arquivo de footer e economizou codigo-->